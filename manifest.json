{
    "manifest_version": 2,
    "name": "MySQL Console",
    "description": "This application provides you 'MySQL Console' shell windows.",
    "version": "1.1.1",
    "author": "Yoichiro Tanaka",
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        {"socket": [
            "tcp-connect:*:*"
        ]},
        "storage"
    ],
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "offline_enabled": true
}
