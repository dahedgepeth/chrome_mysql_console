{
    "manifest_version": 2,
    "name": "MySQL Console",
    "description": "__MSG_extDescription__",
    "version": "1.0.0",
    "author": "Yoichiro Tanaka",
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        {"socket": [
            "tcp-connect:*:*"
        ]},
        "storage"
    ],
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "offline_enabled": false,
    "default_locale": "en"
}
